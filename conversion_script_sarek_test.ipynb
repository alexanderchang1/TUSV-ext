    "import vcfpy\n",
    "\n",
    "\n",
    "def is_cnv_record(rec):\n",
    "    return rec.ID[0][0:3] == 'cnv'\n",
    "\n",
    "\n",
    "def is_sv_record(rec):\n",
    "    return rec.ID[0][0:2] == 'sv'\n",
    "\n",
    "\n",
    "def is_snv_record(rec):\n",
    "    return rec.ID[0][0:3] == 'snv'\n",
